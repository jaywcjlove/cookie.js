{
  "name": "cookiejs",
  "version": "2.0.2",
  "description": "A simple, lightweight JavaScript API for handling browser cookies.",
  "homepage": "https://jaywcjlove.github.io/cookie.js/",
  "main": "dist/cookie.cjs.js",
  "module": "dist/cookie.esm.js",
  "browser": "dist/cookie.js",
  "types": "index.d.ts",
  "files": [
    "dist/*.js",
    "index.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jaywcjlove/cookie.js.git"
  },
  "keywords": [
    "cookiejs",
    "cookie",
    "cookies"
  ],
  "author": "kenny wong",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jaywcjlove/cookie.js/issues"
  },
  "scripts": {
    "prepare": "npm run build",
    "test": "tsbb test",
    "coverage": "tsbb test --coverage",
    "build": "node script/build.js",
    "watch": "node script/watch.js"
  },
  "markdown-to-html": {
    "reurls": {
      "./README-zh.md": "index.zh.html",
      "./README.md": "index.html"
    }
  },
  "devDependencies": {
    "@babel/cli": "7.16.8",
    "@babel/core": "7.16.10",
    "@babel/plugin-proposal-class-properties": "7.16.7",
    "@babel/plugin-proposal-object-rest-spread": "7.16.7",
    "@babel/plugin-transform-async-to-generator": "7.16.8",
    "@babel/preset-env": "7.16.10",
    "@rollup/plugin-babel": "5.3.0",
    "@rollup/plugin-commonjs": "20.0.0",
    "@rollup/plugin-node-resolve": "13.0.5",
    "bannerjs": "2.0.1",
    "colors-cli": "1.0.28",
    "rollup": "2.56.3",
    "rollup-plugin-terser": "7.0.2",
    "tsbb": "3.3.7",
    "zlib": "1.0.5"
  }
}
